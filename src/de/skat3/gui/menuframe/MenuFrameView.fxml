<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.shape.*?>
<?import javafx.scene.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="mainPane" prefHeight="700.0" prefWidth="1300.0"
	style="-fx-min-width: 1300; -fx-min-height: 700;" xmlns="http://javafx.com/javafx/8"
	xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.skat3.gui.menuframe.MenuFrameController">
	<children>
		<SubScene fx:id="subMenu" height="569.0" layoutY="132.0"
			width="1300.0">
			<root>
				<Region />
			</root>
		</SubScene>
		<ImageView fitHeight="700.0" fitWidth="1300.0" opacity="0.73"
			pickOnBounds="true">
			<image>
				<Image url="@../../../../../resources/guifiles/bg.png" />
			</image>
		</ImageView>
		<Pane layoutY="97.0" prefHeight="35.0" prefWidth="1326.0">
			<children>
				<Rectangle fx:id="menuNameLine" arcHeight="5.0" arcWidth="5.0"
					fill="DODGERBLUE" height="2.0" mouseTransparent="true" stroke="BLACK"
					strokeType="INSIDE" translateX="58.0" translateY="30.0" width="130.0" />
				<Pane prefHeight="35.0" prefWidth="1332.0"
					style="-fx-background-color: #000000; -fx-opacity: 20%;" />
				<HBox prefHeight="25.0" prefWidth="1332.0">
					<children>
						<Button fx:id="singleplayerMenuButton" mnemonicParsing="false"
							onAction="#showSingleplayerMenu" onMouseEntered="#handleStartButtonAnimation"
							onMouseExited="#handleEndButtonAnimation" prefHeight="35.0"
							prefWidth="246.0" style="-fx-background-color: transparent;"
							text="SINGLEPLAYER" textFill="#353535">
							<font>
								<Font size="18.0" />
							</font>
						</Button>
						<Button fx:id="multiplayerMenuButton" mnemonicParsing="false"
							onAction="#showMultiplayerMenu" onMouseEntered="#handleStartButtonAnimation"
							onMouseExited="#handleEndButtonAnimation" prefHeight="35.0"
							prefWidth="249.0" style="-fx-background-color: transparent;"
							text="MULTIPLAYER" textFill="#353535">
							<font>
								<Font size="18.0" />
							</font>
						</Button>
						<Button fx:id="statsMenuButton" mnemonicParsing="false"
							onAction="#showStatsMenu" onMouseEntered="#handleStartButtonAnimation"
							onMouseExited="#handleEndButtonAnimation" prefHeight="35.0"
							prefWidth="197.0" style="-fx-background-color: transparent;"
							text="STATS" textFill="#353535">
							<font>
								<Font size="18.0" />
							</font>
						</Button>
						<Button fx:id="optionsMenuButton" minWidth="68.0"
							mnemonicParsing="false" onAction="#showOptionsMenu"
							onMouseEntered="#handleStartButtonAnimation" onMouseExited="#handleEndButtonAnimation"
							prefHeight="35.0" prefWidth="130.0"
							style="-fx-background-color: transparent; -fx-min-width: 100;"
							text="OPTIONS" textFill="#353535">
							<font>
								<Font size="18.0" />
							</font>
						</Button>
					</children>
				</HBox>
			</children>
		</Pane>
		<MenuButton layoutX="915.0" layoutY="14.0" mnemonicParsing="false"
			prefHeight="56.0" prefWidth="360.0"
			style="-fx-background-color: transparent; -fx-border-color: #000000;"
			text="MenuButton">
			<items>
				<MenuItem mnemonicParsing="false" text="Action 1" />
				<MenuItem mnemonicParsing="false" text="Action 2" />
			</items>
		</MenuButton>
	</children>
</AnchorPane>
