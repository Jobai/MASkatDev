<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="653" prefWidth="600.0"
	xmlns="http://javafx.com/javafx/8.0.171"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="de.skat3.gui.resultscreen.RoundResultViewController">
	<children>
		<Pane layoutY="-1.0" prefHeight="80.0" prefWidth="600.0"
			style="-fx-background-color: #404040;">
			<children>
				<Label layoutX="228.0" layoutY="10.0" text="Result"
					textFill="WHITE">
					<font>
						<Font size="49.0" />
					</font>
				</Label>
			</children>
		</Pane>
		<HBox alignment="CENTER" layoutY="112.0" prefHeight="39.0"
			prefWidth="600.0" spacing="60.0"
			style="-fx-background-color: #d60202;">
			<children>
				<CheckBox fx:id="cbSuit" mnemonicParsing="false"
					text="Suit Game" textFill="WHITE" />
				<CheckBox fx:id="cbGrande" mnemonicParsing="false"
					text="Grand" textFill="WHITE" />
				<CheckBox fx:id="cbNull" mnemonicParsing="false"
					text="Null" textFill="WHITE" />
				<CheckBox fx:id="cbNullOvert" mnemonicParsing="false"
					text="Null Overt" textFill="WHITE" />
			</children>
		</HBox>
		<Pane layoutY="491.0" prefHeight="119.0" prefWidth="600.0"
			style="-fx-background-color: F0C808;">
			<children>
				<Label layoutX="52.0" layoutY="40.0" text="Solo Player:">
					<font>
						<Font size="32.0" />
					</font>
				</Label>
				<Label fx:id="pointsSoloPlayer" layoutX="341.0" layoutY="40.0">
					<font>
						<Font size="32.0" />
					</font>
				</Label>
			</children>
		</Pane>
		<Pane layoutY="179.0" prefHeight="281.0" prefWidth="600.0"
			style="-fx-background-color: #086788;">
			<children>
				<GridPane layoutX="34.0" layoutY="14.0" prefHeight="237.0"
					prefWidth="533.0">
					<columnConstraints>
						<ColumnConstraints hgrow="SOMETIMES"
							maxWidth="303.0" minWidth="10.0" prefWidth="293.0" />
						<ColumnConstraints hgrow="SOMETIMES"
							maxWidth="262.0" minWidth="10.0" prefWidth="240.0" />
					</columnConstraints>
					<rowConstraints>
						<RowConstraints minHeight="10.0" prefHeight="30.0"
							vgrow="SOMETIMES" />
						<RowConstraints minHeight="10.0" prefHeight="30.0"
							vgrow="SOMETIMES" />
						<RowConstraints minHeight="10.0" prefHeight="30.0"
							vgrow="SOMETIMES" />
						<RowConstraints minHeight="10.0" prefHeight="30.0"
							vgrow="SOMETIMES" />
						<RowConstraints minHeight="10.0" prefHeight="30.0"
							vgrow="SOMETIMES" />
						<RowConstraints minHeight="10.0" prefHeight="30.0"
							vgrow="SOMETIMES" />
					</rowConstraints>
					<children>
						<Label text="Round:" textFill="WHITE">
							<font>
								<Font size="24.0" />
							</font>
						</Label>
						<Label fx:id="rounds" textFill="WHITE"
							GridPane.columnIndex="1">
							<font>
								<Font size="24.0" />
							</font>
						</Label>
						<Label fx:id="namePlayer1" textFill="WHITE"
							GridPane.rowIndex="2">
							<font>
								<Font size="20.0" />
							</font>
						</Label>
						<Label fx:id="namePlayer2" textFill="WHITE"
							GridPane.rowIndex="3">
							<font>
								<Font size="20.0" />
							</font>
						</Label>
						<Label fx:id="namePlayer3" textFill="WHITE"
							GridPane.rowIndex="4">
							<font>
								<Font size="20.0" />
							</font>
						</Label>
						<Label fx:id="namePlayer4" textFill="WHITE"
							GridPane.rowIndex="5">
							<font>
								<Font size="20.0" />
							</font>
						</Label>
						<Label fx:id="pointsPlayer1" textFill="WHITE"
							GridPane.columnIndex="1" GridPane.rowIndex="2">
							<font>
								<Font size="20.0" />
							</font>
						</Label>
						<Label fx:id="pointsPlayer2" textFill="WHITE"
							GridPane.columnIndex="1" GridPane.rowIndex="3">
							<font>
								<Font size="20.0" />
							</font>
						</Label>
						<Label fx:id="pointsPlayer3" textFill="WHITE"
							GridPane.columnIndex="1" GridPane.rowIndex="4">
							<font>
								<Font size="20.0" />
							</font>
						</Label>
						<Label fx:id="pointsPlayer4" textFill="WHITE"
							GridPane.columnIndex="1" GridPane.rowIndex="5">
							<font>
								<Font size="20.0" />
							</font>
						</Label>
					</children>
				</GridPane>
			</children>
		</Pane>
	</children>
</AnchorPane>
